<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
    </padding>
    <Label fx:id="dialog" lineSpacing="1.5" minHeight="-Infinity" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
        <font>
            <Font name="Helvetica Bold" size="15.0"/>
        </font>
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
        </HBox.margin>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
        <clip>
            <Circle radius="25.0" centerX="30.0" centerY="30.0" />
         </clip>
          <HBox.margin>
            <Insets />
        </HBox.margin>
    </ImageView>
</fx:root>